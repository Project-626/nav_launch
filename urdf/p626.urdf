<robot name="Project_626">
  <material name="base_color">
    <color rgba="0.5 0.2 0.2 0.8" />
  </material>
  <material name="gray">
    <color rgba="0.3 0.3 0.3 1" />
  </material>

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.002" />
      <geometry>
        <box size="0.51 0.36 0.108" />
      </geometry>
      <material name="base_color" />
    </visual>
  </link>

  <link name="laser_link">
    <visual>
      <origin xyz="0 0 -0.055" />
      <geometry>
        <cylinder length="0.13" radius="0.035" />
      </geometry>
      <material name="base_color" />
    </visual>
  </link>

  <link name="imu_link" />

  <joint name="base_to_imu" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
    <origin xyz="-0.075 0 0" />
  </joint>

  <joint name="base_to_laser" type="fixed">
    <parent link="base_link" />
    <child link="laser_link" />
    <origin xyz="-0.075 0 0.12" />
  </joint>
</robot>
